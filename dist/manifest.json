{
  "manifest_version": 2,
  "name": "Quick Shot",
  "version": "1.0.0",
  "description": "Browser extension to quickly navigate between URLs ",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "./icons/icon-16.png",
    "48": "./icons/icon-48.png",
    "128": "./icons/icon-128.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "./icons/icon-48.png",
    "default_popup": "./popup/index.html",
    "default_title": "Quick Shot"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
