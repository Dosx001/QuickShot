    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "sass": "^1.70.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.12",
    "vite-plugin-web-extension": "^4.1.1"
  }
}
